import 'dart:io';
void main()
{
var sentence1;
var sentence2;
var sentence3;
print('Enter a sentence');
sentence1=stdin.readLineSync()!;

sentence2=sentence1;

sentence3=sentence2.split('').reversed.join('');
 if(sentence3==sentence1)
 {
 print("$sentence1 is palindrome");
 }
 else
 {
 print("$sentence1 is not palindrome");
 }



}
